{
  "name": "bundler-test",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "test:manual": "cypress run",
    "build:local-packages": "docker image prune -f && docker container prune -f && docker-compose --profile local-packages build --no-cache",
    "build:test-page": "tsx ./create-test-page.ts",
    "run:vite": "docker-compose --profile run-vite up",
    "run:cli": "docker-compose --profile run-cli up",
    "run:nuxt-ssr": "docker-compose --profile run-nuxt-ssr up",
    "run:nuxt-spa": "docker-compose --profile run-nuxt-spa up",
    "test:vite": "docker-compose --profile vite up --build --exit-code-from cypress_vite",
    "test:cli": "docker-compose --profile cli up --build --exit-code-from cypress_cli",
    "test:nuxt": "yarn test:nuxt-ssr && yarn test:nuxt-spa",
    "test:webpack": "docker-compose --profile webpack up --build --exit-code-from cypress_webpack",
    "test:nuxt-ssr": "docker-compose --profile nuxt-ssr up --build --exit-code-from cypress_nuxt_ssr",
    "test:nuxt-spa": "docker-compose --profile nuxt-spa up --build --exit-code-from cypress_nuxt_spa",
    "test": "yarn build:test-page && yarn build:local-packages && yarn test:vite && yarn test:cli && yarn test:nuxt && yarn test:webpack"
  },
  "dependencies": {
    "cypress": "^10.11.0"
  }
}
